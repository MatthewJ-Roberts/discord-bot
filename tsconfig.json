{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Node 16",
    "include": ["./*"],
    "exclude": ["node_modules"],
    "compilerOptions": {
        "lib": ["es2020"],
        "module": "es2020",
        "target": "es2020",
        "outDir": "dist",
        "moduleResolution": "node",
        "noImplicitAny": false,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": false,
        "alwaysStrict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    }  
    
}